<launch>

  <!-- <group ns="/dvrk_psm"> -->
  <group>

    <arg name="robot_name" default="one"/>

    <rosparam>
      <!-- if connect to dv hardware, update this source list  -->
      source_list: [/dvrk_psm/joint_states_command]
    </rosparam>

    <node name="psm_kinematics"
          pkg="dvrk_kinematics"
          type="dvrk_psm_logic"
          output="screen">
      <param name="robot_name" value="$(arg robot_name)"/>
    </node>
    
    <include file="$(find dvrk_model)/launch/psm_one_rviz.launch"/>
  </group>

</launch>


